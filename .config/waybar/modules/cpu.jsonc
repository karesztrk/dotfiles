{
  "cpu": {
    "states": {
      "normal": 1,
      "elevated": 25,
      "warning": 50,
      "critical": 75
    },
    "interval": 10,
    "format": "󰍛 {usage}%",
    "rotate": 0,
    "format-alt": "{icon0}{icon1}{icon2}{icon3}",
    "format-icons": [
      "▁",
      "▂",
      "▃",
      "▄",
      "▅",
      "▆",
      "▇",
      "█"
    ]
  }
}
